# Supabase Project Settings - See https://supabase.com/docs/guides/with-nextjs#get-the-api-keys
# Find these in your Supabase project dashboard -> Settings -> API
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# **NEVER** expose the following in the browser
# Used for server-side operations and bypassing RLS (e.g., migrations, admin tasks)
SUPABASE_SERVICE_ROLE_KEY=

# Used for JWT verification (optional, but recommended for security)
# Can be found in Supabase project dashboard -> Settings -> API -> JWT Settings
SUPABASE_JWT_SECRET=
